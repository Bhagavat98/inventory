<script type="text/javascript">
jQuery(document).ready(function($){
	$(".head-menu .toggle").click( function() {
		if ($('.head-menu .menu').hasClass('open')) {
			$('.head-menu .menu').slideUp(500, function() {
				$('.head-menu .menu').removeClass('open');
			});
		} else {
			$('.head-menu .menu').slideDown(500, function() {
				$('.head-menu .menu').addClass('open');
			});
		}
	});
});
